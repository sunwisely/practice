#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main (void){
  int input=0;
  printf("do you want to study hard now?please choose(1/0)\n");
  scanf("%d",&input);
  if(input!=1&&input!=0)
    printf("it is wrong");
  else
    if(input==1)
      printf("you will success");
    else
      printf("i am shamed with you");
      
  return EXIT_SUCCESS;
  

}
